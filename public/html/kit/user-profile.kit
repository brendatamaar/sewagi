<!doctype html>
<html lang="en">
<!-- @import "_list-property-header.kit" -->

<body>
    <div id="dashboard">
        <div id="page-wrapper" class="d-flex">
            <!-- @import "_sidebar-dashboard.kit" -->
            <div id="page-container">
                <div class="nav-dashboard d-flex align-items-center justify-content-between">
                    <ul class="nav" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="true">PROFILE</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="digital-id-tab" data-toggle="tab" href="#digital-id" role="tab" aria-controls="digital-id" aria-selected="false">DIGITAL IDENTITY</a>
